<h1>Gallery</h1>

<div class="container">
  
  <% @projects.each do |project| %>
    <div class="col-xs-12, col-sm-6, col-md-4, col-lg-3">
      <dl>
        <% project.each_pair do |key, value| %>
          <dt><%= key %></dt>
          <dd><%= value %></dd>
        <% end %> 
      </dl>
      <%= image_tag project["covers"]["404"] %>
    </div>
  <% end %> 
  
</div>